<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="org.framestudy.spring_hibernate.personmag.beans.PersonInfo" catalog="spring_hibernate" table="t_person_info">
		<id name="id" type="java.lang.Long">
			<column name="id"></column>
			<generator class="identity"></generator>
		</id>
		<property name="msg" type="java.lang.String">
			<column name="person_msg" length="120"></column>
		</property>
		<property name="idCard" type="java.lang.String">
			<column name="idcard" length="18"></column>
		</property>
		<property name="fkPersonId" type="long">
			<column name="fk_person_id"></column>
		</property>
	</class>
</hibernate-mapping>